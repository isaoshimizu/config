[client]
port     = 3306
socket   = /tmp/mysql.sock

[mysqld]
default-storage-engine   = InnoDB
port                     = 3306
socket                   = /tmp/mysql.sock
innodb_file_per_table    = OFF
read_rnd_buffer_size = 2M
sort_buffer_size = 4M
innodb_read_io_threads = 16
innodb_write_io_threads = 16
